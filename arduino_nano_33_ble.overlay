&i2c1 {
    clock-frequency = <I2C_BITRATE_FAST>;
    hts221@5f {
        compatible = "st,hts221";
        label = "HTS221";
        reg = <0x5f>;
    };
    lps22hb-press@5c {
        compatible = "st,lps22hb-press";
        label = "LPS22HB";
        reg = <0x5c>;
    };
};

&spi2 {
    cs-gpios = <&gpio1 2 GPIO_ACTIVE_LOW>;
    sdhc0: sdhc@0 {
        compatible = "zephyr,mmc-spi-slot";
        reg = <0>;
        label = "SDHC0";
        spi-max-frequency = <24000000>;
    };
};